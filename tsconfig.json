{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": "./",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@content/*": [
                "src/content/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ]
        }
    }
}
